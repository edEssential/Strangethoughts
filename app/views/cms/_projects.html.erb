<div class="cmsWrapper">
	<div class="releases">
		<div class="divHeader">Projects</div>
		<div class="tableWrapper">
			<div class="tableContent">
				<% if @projects.count > 0 %>
					<table width="100%">
						<tr class="tableHeader">
							<th>TITLE</th>
							<th>RESULT</th>
						</tr>
						<tr><hr class="ruler"/></tr>
						<% @projects.each do |p| %>
							<tr class="tableRow">
								<td width="20%"><%= p.title %></td>
								<td width="76%"><%= truncate (p.result), length: 90, :separator => '.' %></td>
								<td width="2%"><%= link_to image_tag('edit2.png'), edit_project_path(p), class: "delete" %></td>
								<td width="2%"><%= link_to image_tag('delete2.png'), p, class: "delete", :confirm => 'Are you sure you want to delete this release?', :method => :delete %></td>
							</tr>
						<% end %>
					</table>
				<% else %>
					<table width="100%"><tr class="tableRow empty"><td><em>YOU HAVE NO PROJECTS YET... ADD SOME BELOW ?!</em></td></tr></table>
				<% end %>
			</div>
		</div>
	</div>
	<h6>
	<input type="hidden" value="videos" class="modal-model-type">
	<h6><%= link_to 'Add new project', new_project_path, :class => "darkButton2" %></h6>
</div>